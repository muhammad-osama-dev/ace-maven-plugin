

BROKER SCHEMA CommonCalc

/* -----------------------------------------------------------------------------
* calculate sum
* -----------------------------------------------------------------------------
*/
CREATE PROCEDURE CalculateSum(
IN valueA REFERENCE,
IN valueB REFERENCE,
OUT sum INTEGER)
BEGIN
	SET sum = valueA + valueB;
END;



